# Driver interface for ATHEROS-AR600x driver

CONFIG_DRIVER_AR6000=y
AR6K_ROOT=$(WORKAREA)
CFLAGS += -I$(AR6K_ROOT)/include
CFLAGS += -I$(AR6K_ROOT)/host/include
CFLAGS += -I$(AR6K_ROOT)/host/os/linux/include
CFLAGS += -I$(AR6K_ROOT)/host/wlan/include

CONFIG_EAP=y
CONFIG_WPS=y
CONFIG_WPS2=y
CONFIG_WPS_UPNP=y
